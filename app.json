{
  "expo": {
    "name": "wakeupbrah",
    "slug": "wakeupbrah",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wakeupbrah",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.elgabrielcisneros.wakeupbrah"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Manrope-Regular.ttf",
            "./assets/fonts/Inter_18pt-Regular.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Inter",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Inter_18pt-Bold.ttf",
                    "weight": "700",
                    "style": "bold"
                  },
                  {
                    "path": "./assets/fonts/Inter_18pt-Italic.ttf",
                    "weight": "400",
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/Inter_18pt-Light.ttf",
                    "weight": "200",
                    "style": "light"
                  },
                  {
                    "path": "./assets/fonts/Inter_18pt-Medium.ttf",
                    "weight": "500",
                    "style": "medium"
                  },
                  {
                    "path": "./assets/fonts/Inter_18pt-SemiBold.ttf",
                    "weight": "600",
                    "style": "semibold"
                  }
                ]
              },
              {
                "fontFamily": "Manrope",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Manrope-Bold.ttf",
                    "weight": "700"
                  },
                  {
                    "path": "./assets/fonts/Manrope-SemiBold.ttf",
                    "weight": "600"
                  },
                  {
                    "path": "./assets/fonts/Manrope-Medium.ttf",
                    "weight": "500",
                    "style": "medium"
                  },
                  {
                    "path": "./assets/fonts/Manrope-Light.ttf",
                    "weight": "300",
                    "style": "light"
                  },
                  {
                    "path": "./assets/fonts/Manrope-ExtraLight.ttf",
                    "weight": "200"
                  }
                ]
              }
            ]
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "62e60af8-0161-4662-bafa-2290663b127a"
      }
    },
    "owner": "brah-tek"
  }
}
